{
    "author": "Junfeng Qiao, Miki Bonacci, Giovanni Pizzi",
    "author_email": "developers@aiida.net",
    "classifiers": [
        "Environment :: Plugins",
        "Framework :: AiiDA",
        "Intended Audience :: Science/Research",
        "Programming Language :: Python :: 3",
        "Programming Language :: Python :: 3.8",
        "Programming Language :: Python :: 3.9",
        "Programming Language :: Python :: 3.10",
        "Topic :: Scientific/Engineering :: Physics"
    ],
    "description": "Plugin to combine Wannier90 interpolations with GW corrections computed by Yambo",
    "entry_points": {
        "aiida.parsers": [],
        "aiida.workflows": [
            "aiida_yambo_wannier90.workflow = aiida_yambo_wannier90.workflow:YamboWannier90WorkChain"
        ]
    },
    "extras_require": {
        "docs": [
            "sphinx",
            "docutils",
            "sphinx-copybutton~=0.3.0",
            "sphinx-book-theme~=0.1.0",
            "sphinx-click~=2.7.1"
        ],
        "pre-commit": [
            "pre-commit~=2.2",
            "yapf",
            "pylint>=2.7.0"
        ],
        "tests": [
            "pgtest~=1.3",
            "pytest~=6.0",
            "pytest-cov",
            "pytest-regressions~=1.0"
        ]
    },
    "install_requires": [
        "aiida-core~=1.6",
        "aiida-wannier90-workflows>=1.0",
        "yambo-aiida"
    ],
    "keywords": "aiida yambo wannier90 workflows",
    "license": "Private code",
    "name": "aiida-yambo-wannier90",
    "python_requires": ">=3.8",
    "url": "https://github.com/epfl-theos/aiida-yambo-wannier90",
    "version": "0.0.1"
}
